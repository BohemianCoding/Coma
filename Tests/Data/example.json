	{
        "passes":["header"],

		"types":
		{
			"base" : "standardTypes",
			"items" :
			{
				"NSString" :
				{
					"metatype" : "Object",
				},

				"NSInteger" :
				{
					"metatype" : "Basic",
				},

				"CustomClass" :
				{
					"metatype" : "DeepCopyObject",
                    "requires" : "CustomClass.h",
				}
			}
		},
	
		"metas":
		{
			"base" : "standardTemplates",
			"items" :
			{
		        "Basic" :
		        {
		            "setter": "basicSetter.template",
		            "getter": "basicGetter.template",
		            "copy": "basicCopy.template"
				},
		        "Object" :
		        {
		            "setter": "objectSetter.template",
		            "getter": "objectGetter.template",
		            "copy": "object.template"
				},
		        "DeepCopyObject" :
		        {
		            "setter": "objectSetter.template",
		            "getter": "objectGetter.template",
		            "copy": "deepCopy.template"
				},
			}
		},
	
		"classes":
		{
			"Person" :
			{
			    "super": "BaseClass",
       
			    "properties":
			    {
					"name" :
			        {
			            "type" : "NSString",
			            "undoName" : "Name",
						"description" : "Person's name"
			        },
				
					"age" :
			        {
			            "type" : "NSInteger",
			            "undoName": "Age",
						"description" : "Person's age"
					},
				
					"custom" :
			        {
			            "type" : "CustomClass",
			            "undoName": "Custom",
						"description" : "Property with custom object that needs to be deep copied for some reason"
					}
				
				}		    
			}
		}
	}

