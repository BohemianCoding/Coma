	{
        "templates":["header", "source"],

		"types":
		{
			"base" : "standardTypes",
			"items" :
			{
				"NSString" :
				{
					"metatype" : "Object",
				},

				"NSInteger" :
				{
					"metatype" : "Basic",
				},

				"CustomClass" :
				{
					"metatype" : "DeepCopyObject",
                    "requires" : "CustomClass.h",
				}
			}
		},
	
		"metas":
		{
			"base" : "standardTemplates",
			"items" :
			{
		        "Basic" :
		        {
                    "templates" :
                    {
                        "getter": "basicGetter",
                        "setter": "basicSetter",
                        "copier" : "basicCopy",
                        "encoder" : "basicEncode",
                        "decoder" : "basicDecode",
                    },
                    "mode" : "assign",
                    "basic" : true
				},
		        "Object" :
		        {
                    "templates" :
                    {
                        "getter": "objectGetter",
                        "setter": "objectSetter",
                        "copier" : "objectCopy",
                        "encoder" : "objectEncode",
                        "decoder" : "objectDecode",
                    },
				},
		        "DeepCopyObject" :
		        {
                    "templates" :
                    {
                        "getter": "objectGetter",
                        "setter": "objectSetter",
                        "copier" : "deepCopy",
                        "encoder" : "objectEncode",
                        "decoder" : "objectDecode",
                    },
				},
			}
		},
	
		"classes":
		{
			"Person" :
			{
			    "super": "BaseClass",
       
			    "properties":
			    {
					"name" :
			        {
			            "type" : "NSString",
			            "undoName" : "Name",
						"description" : "Person's name"
			        },
				
					"age" :
			        {
			            "type" : "NSInteger",
			            "undoName": "Age",
						"description" : "Person's age"
					},
				
					"custom" :
			        {
			            "type" : "CustomClass",
			            "undoName": "Custom",
						"description" : "Property with custom object that needs to be deep copied for some reason"
					}
				
				}		    
			}
		}
	}

