	{
        "templates":["header", "source"],

		"types":
		{
			"base" : "standardTypes",
			"items" :
			{
				"CustomClass" :
				{
					"metatype" : "DeepCopyObject",
                    "requires" : "CustomClass.h",
				}
			}
		},
	
		"metas":
		{
			"base" : "standardMetas",
			"items" :
			{
		        "DeepCopyObject" :
		        {
                    "templates" :
                    {
                        "getter": "objectGetter",
                        "setter": "objectSetter",
                        "copier" : "deepCopy",
                        "mutableCopier" : "deepMutableCopy",
                        "encoder" : "objectEncode",
                        "decoder" : "objectDecode",
                    },
				},
			}
		},
	
		"classes":
		{
			"Person" :
			{
			    "super": "BaseClass",
                "generateMutableCopy" : true,
			    "properties":
			    {
					"name" :
			        {
			            "type" : "NSString",
			            "undoName" : "Name",
						"description" : "Person's name"
			        },
				
					"age" :
			        {
			            "type" : "NSInteger",
			            "undoName": "Age",
						"description" : "Person's age"
					},

					"custom" :
			        {
			            "type" : "CustomClass",
			            "undoName": "Custom",
						"description" : "Property with custom object that needs to be deep copied for some reason"
					}
				
				}    
			}
		}
	}

