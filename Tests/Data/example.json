	{
        "templates":["header", "source"],

		"types":
		{
			"base" : "standardTypes",
			"items" :
			{
				"CustomClass" :
				{
					"metatype" : "DeepCopyObject",
                    "requires" : "CustomClass.h",
				}
			}
		},
	
		"metas":
		{
			"base" : "standardMetas",
			"items" :
			{
		        "DeepCopyObject" :
		        {
                    "templates" :
                    {
                        "getter": "objectGetter",
                        "setter": "objectSetter",
                        "copier" : "deepCopy",
                        "mutableCopier" : "deepMutableCopy",
                        "encoder" : "objectEncode",
                        "decoder" : "objectDecode",
                    },
				},
			}
		},
	
		"classes":
		{
			"Example" :
			{
			    "super":
                {
                    "class" : "BaseClass",
                    "import" : "BaseClass.h"
                },
                "protocols" : "<NSCoding, NSCopying>",
                "generateMutableCopy" : false,
			    "properties":
			    {
					"string" :
			        {
			            "type" : "NSString",
						"description" : "NSString example"
			        },
				
					"integer" :
			        {
			            "type" : "NSInteger",
						"description" : "NSInteger example"
					},

                    "unsignedInteger" :
			        {
			            "type" : "NSUInteger",
						"description" : "NSUInteger example"
					},

                    "real" :
			        {
			            "type" : "CGFloat",
						"description" : "CGFloat example"
					},

                    "rect" :
			        {
			            "type" : "NSRect",
						"description" : "NSRect example"
					},

                    "point" :
			        {
			            "type" : "NSPoint",
						"description" : "NSPoint example"
					},

                    "date" :
			        {
			            "type" : "NSDate",
						"description" : "NSDate example"
					},

                    "boolean" :
			        {
			            "type" : "BOOL",
						"description" : "BOOL example"
					},

					"custom" :
			        {
			            "type" : "CustomClass",
						"description" : "Property with custom object that needs to be deep copied for some reason"
					}
				
				}    
			}
		}
	}

