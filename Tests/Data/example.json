	{
        "passes":["header", "source"],

		"types":
		{
			"base" : "standardTypes",
			"types" :
			{
				"CustomClass" :
				{
					"metatype" : "DeepCopyObject",
					"import" : "CustomClass.h"
				}
			}
		},
	
		"templates":
		{
			"base" : "standardTemplates",
			"templates" :
			{
		        "DeepCopyObject" : 
		        {
		            "setter": "objectSetter.template",
		            "getter": "objectGetter.template",
		            "copy": "deepCopy.template"
				},
			}
		},
	
		"classes":
		{
			"Person" :
			{
			    "super": "BaseClass",
       
			    "properties":
			    {
					"name" :
			        {
			            "type" : "NSString",
			            "undoName" : "Name",
						"description" : "Person's name"
			        },
				
					"age" :
			        {
			            "type" : "NSInteger",
			            "undoName": "Age",
						"description" : "Person's age"
					},
				
					"custom" :
			        {
			            "type" : "CustomClass",
			            "undoName": "Custom",
						"description" : "Property with custom object that needs to be deep copied for some reason"
					}
				
				}		    
			}
		}
	}

